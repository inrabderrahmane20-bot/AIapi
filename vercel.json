{
  "version": 2,
  "builds": [
    {
      "src": "api/AI.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/AI.py"
    }
  ],
  "env": {
    "FLASK_DEBUG": "false",
    "CACHE_TTL": "3600",
    "MAX_IMAGE_WORKERS": "8",
    "REQUEST_TIMEOUT": "10",
    "FLASK_PORT": "5000",
    "MAP_TILE_PROVIDER": "openstreetmap",
    "CACHE_DIR": "/tmp/diskcache",
    "PRELOAD_TOP_CITIES": "12",
    "LOCAL_CACHE_FILE": "/tmp/cities_data.json",
    "MAX_WIKIMEDIA_FILES_TO_SCAN": "50",
    "MAX_IMAGES_PER_REQUEST": "6",
    "WIKIMEDIA_RETRY_ATTEMPTS": "3"
  }
}